                        <ui:IntegerField label="MinTrees" value="1" name="MinTrees" style="margin-right: 2px; width: 211px;" />
                        <ui:IntegerField label="LakeCount" value="1" name="LakeCount" style="margin-right: 2px; width: 211px;" />
                        <ui:IntegerField label="MapSize" value="150" name="MapSize" style="margin-right: 2px; width: 211px;" />
