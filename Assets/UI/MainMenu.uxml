            <ui:GroupBox style="flex-direction: row; justify-content: space-around; align-self: auto;">
                <ui:GroupBox name="Animals" style="width: 276px;">
                    <ui:GroupBox name="Reproduction" style="height: 110px; width: 211px;">
                        <ui:Label tabindex="-1" text="Reproduction" parse-escape-sequences="true" display-tooltip-when-elided="true" style="-unity-font-style: bold;" />
                        <ui:RadioButton label="Sexual" name="Sexual" value="true" />
                        <ui:RadioButton label="Asexual" name="Asexual" />
                    </ui:GroupBox>
                    <ui:GroupBox name="Other" style="width: 267px;">
                        <ui:Toggle label="Predation" name="Predation" />
                        <ui:Toggle label="Random Spawn" name="RandomSpawnPoint" />
                        <ui:IntegerField label="Initial Animals" value="150" name="InitialAnimals" style="margin-right: 2px; width: 211px;" />
                        <ui:IntegerField label="Max Animals" value="150" name="MaxAnimals" style="margin-right: 2px; width: 211px;" />
                        <ui:IntegerField label="Reproduction Energy" value="1" name="ReproductionEnergy" style="margin-right: 2px; width: 211px;" />
                    </ui:GroupBox>
                </ui:GroupBox>
                <ui:GroupBox name="Environment" style="width: 276px; flex-direction: column;">
